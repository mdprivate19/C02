ex07

#include <stdio.h>
int	main()
{
	char str[] = "00%^(^&^THFYYUfhrghrg3654";

	printf("%s", ft_strupcase(str));
	return (0);
}

ex08

#include <stdio.h>
int	main()
{
	char	str[] = "THJR^%*%*YFYJYfDRGdhdeufhe645";

	printf("%s", ft_strlowcase(str));
	return (0);
}

